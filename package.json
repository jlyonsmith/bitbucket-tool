{
  "name": "@jlyonsmith/bitbucket-tool",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Bitbucket CLI",
  "scripts": {
    "build": "babel src -d dist -s --ignore *.test.js",
    "debug:bucket": "babel-node --inspect-brk src/bucket.js",
    "test": "jest",
    "test:debug": "babel-node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "bin": {
    "bit": "dist/bitbucket.js"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/bitbucket-tool.git"
  },
  "keywords": [
    "bitbucket",
    "git",
    "cli",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/bitbucket-tool/issues"
  },
  "homepage": "https://github.com/jlyonsmith/bitbucket-tool#readme",
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "chalk": "^2.1.0",
    "command-exists": "^1.2.8",
    "consul": "^0.34.0",
    "flat": "^4.1.0",
    "json5": "^0.5.1",
    "minimist": "^1.2.0",
    "opn": "^5.4.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest-cli": "^23.6.0"
  }
}
