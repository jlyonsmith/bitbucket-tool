{
  "name": "@johnls/git-extra",
  "version": "3.0.0",
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Extra git commands for working with BitBucket and GitHub",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist -s --ignore **/*.test.js",
    "debug:git-extra": "babel-node --inspect-brk src/git-extra.js",
    "test": "jest --coverage",
    "test:debug": "babel-node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "deploy": "npm publish"
  },
  "bin": {
    "git-extra": "dist/git-extra.js"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/git-extra.git"
  },
  "keywords": [
    "bitbucket",
    "github",
    "git",
    "cli",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/git-extra/issues"
  },
  "homepage": "https://github.com/jlyonsmith/git-extra#readme",
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "chalk": "^2.4.2",
    "command-exists": "^1.2.8",
    "minimist": "^1.2.0",
    "opn": "^5.4.0",
    "tmp-promise": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "jest-cli": "^24.1.0"
  }
}
